<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Haiping Chen</Authors>
    <Company>Personal</Company>
    <Description>Custom Entity Foundation (CEF) is a high level customized entity library wirtten in C# .Net Core. CEF is with high flexible &amp; business oriented abstract layer. It could be used in any .net project to serve as an entity service.</Description>
    <Copyright>Personal</Copyright>
    <PackageProjectUrl>https://github.com/Haiping-Chen/CustomEntityFoundation</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Haiping-Chen/CustomEntityFoundation</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageReleaseNotes>Bug fixed.</PackageReleaseNotes>
    <PackageTags>EntityFramework, EntityFrameworkCore, Custom Entity, Content Management</PackageTags>
    <Version>2.3.3</Version>
    <AssemblyVersion>2.3.3.0</AssemblyVersion>
    <FileVersion>2.3.3.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Utilities\JObjectHelper.cs" />
    <Compile Remove="Utilities\TypeHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotNetToolkit" Version="1.7.0" />
    <PackageReference Include="EntityFrameworkCore.BootKit" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\" />
    <Folder Include="Views\" />
  </ItemGroup>

</Project>
